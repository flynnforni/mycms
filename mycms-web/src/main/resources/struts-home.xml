        <action name="index" class="com.flynn.mycms.web.common.HomeAction">
            <result>/WEB-INF/vm/home/home.vm</result>
        </action>
        <action name="topFrame" method="frame" class="com.flynn.mycms.web.common.HomeAction">
            <result>/WEB-INF/vm/home/topFrame.vm</result>
