        <action name="leftFrame" method="frame" class="com.flynn.mycms.web.common.HomeAction">
            <result>/WEB-INF/vm/home/leftFrame.vm</result>
            </action>
        <action name="welcome" method="welcome" class="com.flynn.mycms.web.common.HomeAction">
